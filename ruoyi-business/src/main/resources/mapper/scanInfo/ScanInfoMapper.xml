<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.scanInfo.mapper.ScanInfoMapper">
    
    <resultMap type="ScanInfo" id="ScanInfoResult">
        <result property="id"    column="id"    />
        <result property="scanTime"    column="scan_time"    />
        <result property="isForeigner"    column="is_foreigner"    />
        <result property="tag"    column="tag"    />
        <result property="systemSource"    column="system_source"    />
        <result property="companyName"    column="company_name"    />
        <result property="name"    column="name"    />
        <result property="card"    column="card"    />
        <result property="gender"    column="gender"    />
        <result property="birth"    column="birth"    />
        <result property="regionCode"    column="region_code"    />
        <result property="phone"    column="phone"    />
        <result property="color"    column="color"    />
        <result property="scanMethod"    column="scan_method"    />
        <result property="scanLatitude"    column="scan_latitude"    />
        <result property="scanLongitude"    column="scan_longitude"    />
        <result property="deleted"    column="deleted"    />
    </resultMap>

    <sql id="selectScanInfoVo">
        select id, scan_time, is_foreigner, tag, system_source, company_name, name, card, gender, birth, region_code, phone, color, scan_method, scan_latitude, scan_longitude, deleted from sh_scan_info
    </sql>

    <select id="selectScanInfoList" parameterType="ScanInfo" resultMap="ScanInfoResult">
        <include refid="selectScanInfoVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="gender != null "> and gender = #{gender}</if>
            <if test="params.beginBirth != null and params.beginBirth != '' and params.endBirth != null and params.endBirth != ''"> and birth between #{params.beginBirth} and #{params.endBirth}</if>
            <if test="regionCode != null  and regionCode != ''"> and region_code = #{regionCode}</if>
            <if test="phone != null  and phone != ''"> and phone = #{phone}</if>
        </where>
    </select>
    
    <select id="selectScanInfoById" parameterType="Long" resultMap="ScanInfoResult">
        <include refid="selectScanInfoVo"/>
        where id = #{id}
    </select>

    <insert id="insertScanInfo" parameterType="ScanInfo" useGeneratedKeys="true" keyProperty="id">
        insert into sh_scan_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="scanTime != null">scan_time,</if>
            <if test="isForeigner != null">is_foreigner,</if>
            <if test="tag != null">tag,</if>
            <if test="systemSource != null">system_source,</if>
            <if test="companyName != null">company_name,</if>
            <if test="name != null">name,</if>
            <if test="card != null">card,</if>
            <if test="gender != null">gender,</if>
            <if test="birth != null">birth,</if>
            <if test="regionCode != null">region_code,</if>
            <if test="phone != null">phone,</if>
            <if test="color != null">color,</if>
            <if test="scanMethod != null">scan_method,</if>
            <if test="scanLatitude != null">scan_latitude,</if>
            <if test="scanLongitude != null">scan_longitude,</if>
            <if test="deleted != null">deleted,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="scanTime != null">#{scanTime},</if>
            <if test="isForeigner != null">#{isForeigner},</if>
            <if test="tag != null">#{tag},</if>
            <if test="systemSource != null">#{systemSource},</if>
            <if test="companyName != null">#{companyName},</if>
            <if test="name != null">#{name},</if>
            <if test="card != null">#{card},</if>
            <if test="gender != null">#{gender},</if>
            <if test="birth != null">#{birth},</if>
            <if test="regionCode != null">#{regionCode},</if>
            <if test="phone != null">#{phone},</if>
            <if test="color != null">#{color},</if>
            <if test="scanMethod != null">#{scanMethod},</if>
            <if test="scanLatitude != null">#{scanLatitude},</if>
            <if test="scanLongitude != null">#{scanLongitude},</if>
            <if test="deleted != null">#{deleted},</if>
         </trim>
    </insert>

    <update id="updateScanInfo" parameterType="ScanInfo">
        update sh_scan_info
        <trim prefix="SET" suffixOverrides=",">
            <if test="scanTime != null">scan_time = #{scanTime},</if>
            <if test="isForeigner != null">is_foreigner = #{isForeigner},</if>
            <if test="tag != null">tag = #{tag},</if>
            <if test="systemSource != null">system_source = #{systemSource},</if>
            <if test="companyName != null">company_name = #{companyName},</if>
            <if test="name != null">name = #{name},</if>
            <if test="card != null">card = #{card},</if>
            <if test="gender != null">gender = #{gender},</if>
            <if test="birth != null">birth = #{birth},</if>
            <if test="regionCode != null">region_code = #{regionCode},</if>
            <if test="phone != null">phone = #{phone},</if>
            <if test="color != null">color = #{color},</if>
            <if test="scanMethod != null">scan_method = #{scanMethod},</if>
            <if test="scanLatitude != null">scan_latitude = #{scanLatitude},</if>
            <if test="scanLongitude != null">scan_longitude = #{scanLongitude},</if>
            <if test="deleted != null">deleted = #{deleted},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteScanInfoById" parameterType="Long">
        delete from sh_scan_info where id = #{id}
    </delete>

    <delete id="deleteScanInfoByIds" parameterType="String">
        delete from sh_scan_info where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>